{
  "configurations": [
    {
      "args": [
        "test",
        "tests/integration/"
      ],
      "autoStartBrowser": false,
      "django": true,
      "justMyCode": false,
      "name": "Run Tests - Python Debugger Django",
      "program": "${workspaceFolder}/tests/integration/manage.py",
      "python": "${workspaceFolder}/tests/integration/venv/bin/python",
      "request": "launch",
      "type": "debugpy"
    },
    {
      "console": "integratedTerminal",
      "justMyCode": false,
      "name": "Run Current File - Python Debugger",
      "program": "${file}",
      "request": "launch",
      "type": "debugpy"
    },
    {
      "console": "integratedTerminal",
      "justMyCode": false,
      "name": "Python: Debug Unit Tests",
      "purpose": [
        "debug-test"
      ],
      "request": "launch",
      "type": "debugpy"
    }
  ],
  "version": "0.2.0"
}
